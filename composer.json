{
    "name": "diego-drese/nova-backup-manager",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laravel/nova": "^5.0",
        "spatie/laravel-backup": "^9.2"
    },
    "require-dev": {
        "laravel/nova-devtool": "^1.7"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "DiegoDrese\\NovaBackupManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DiegoDrese\\NovaBackupManager\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
